		box-shadow: 0 2px 4px 0 rgba(0,0,0,.2), 0 0 0 1px rgba(185,187,190,.1);
	}
	div[id^="popout"] div[class^="container"] {
		box-shadow: 0 4px 8px rgba(0,0,0,.25);
	}

	/* Gradient */
	#user-settings div[class^="sidebarRegionScroller"],
	#server-settings div[class^="sidebar"],
	div[class^="sidebar"] > nav[aria-label],
	main[class^="chatContent"] + div[class^="members"] {
		background-image: linear-gradient(
			145deg,
			var(--background-secondary),
			var(--background-secondary-gradient)
		);
	}
	div[class^="menu"] > div {
		border-top: 2px solid;
		border-image: linear-gradient(
			to right,
			var(--background-secondary-gradient),
			var(--background-secondary)
		) 1;
	}

	/* Transparent background and blur, mostly eye candy */
	.theme-light {
		--background-floating: var(--background-transparent-primary) !important;
		--background-transparent-primary: rgba(255,255,255,.8);
		--background-transparent-secondary: rgba(255,255,255,.75);
		--background-transparent-secondary-alt: rgba(255,255,255,.7);
		--background-transparent-tertiary: rgba(255,255,255,.25);
	}
	.theme-dark {
		--background-floating: var(--background-transparent-primary) !important;
		--background-transparent-primary: rgba(0,0,0,.8);
		--background-transparent-secondary: rgba(0,0,0,.75);
		--background-transparent-secondary-alt: rgba(0,0,0,.7);
		--background-transparent-tertiary: rgba(0,0,0,.25);
	}

	div[role="dialog"]:not([class]) > div,
	div[aria-label="Pinned Messages"] div[class^="header"],
	div[aria-label="Recent Mentions"] div[class^="header"] {
		background-color: var(--background-transparent-primary) !important;
	}
	div[class^="messageGroupWrapper"],
	div[class^="messageContainer"],
	div[class^="channelTextArea"] > div[class^="autocomplete"] {
		background-color: var(--background-transparent-secondary) !important;
	}
	div[class^="channelHeader"] {
		background-color: var(--background-transparent-secondary-alt) !important;
	}
	div[class^="messageGroupWrapper"] {
		border-color: var(--background-transparent-secondary) !important;
	}
	div[class^="unreadTop"] > div,
	div[class*="tooltipPrimary"] {
		opacity: 1 !important;
		background-color: var(--background-floating) !important;
		backdrop-filter: blur(6px);
	}
	div[class^="unreadTop"] > div {
		color: var(--text-normal) !important;
	}
	div[class^="autocomplete"],
	div[class^="root"],
	div[class^="menu"] {
		backdrop-filter: blur(24px);
	}

	/* Active item */
	div[class*="modeSelected"] div[class^="content"],
	a[class*="container"][class*="selected"],
	div[class^="side"] [class^="item"][class*="selected"],
	div[class*="selected"] div[class^="layout"],
	#bd-settings-sidebar .ui-tab-bar-item.selected {
		background-color: var(--background-modifier-selected) !important;
		color: var(--interactive-active) !important;
		box-shadow: inset 2px 0 0 var(--logo-primary);
	}

	/* Recent mentions */
	.theme-dark div[aria-label="Inbox"] svg {
		background-size: 24px !important;

		&:hover {
			filter: brightness(1.25);
		}

		path {
			display: none;
		}
	}
	/*.theme-light div[aria-label="Inbox"] svg {
		background: url(data:image/svg+xml;base64,);
	}
	.theme-dark div[aria-label="Inbox"] svg {
		background: url(data:image/png;base64,)
	}*/

	div[aria-label="Recent Mentions"] {
		div[class^="tab"][class*="active"] {
			height: unset !important;
			margin: 0 !important;
			padding: 0 !important;
			letter-spacing: -9999px;
			color: transparent !important;
			background-color: transparent !important;

			&::after {
				content: 'Recent Mentions';
				letter-spacing: 0 !important;
				color: var(--header-primary);
			}
		}
		div[class^="tabBar"] div[class^="tab"]:last-child {
			display: none;
		}
		div[class^="header"] {
			box-shadow: var(--elevation-low) !important;
		}
	}
	div[class^="channelHeader"] {
		box-shadow: var(--elevation-low);
		padding-right: 16px;
		position: unset !important;
	}

	/* Enable outline */
	/*a,button,div,input,select,span,strong,textarea {
		outline: revert !important;
	}

	/* Emojis (cozy) */
	.emoji.jumboable {
		width: 1.975em;
		height: 1.975em;
		min-height: 1.975em;
	}

	/* No connection */
	#app-mount > div[class^="app"] > div[class^="container"][class*="fixClipping"] {
		backdrop-filter: blur(36px);
	}
	video + div[class^="text"] > div:first-child {
		visibility: hidden;

		&::before {
			content: 'stfu lol';
			margin-left: 78px;
			color: var(--logo-primary);
			visibility: visible;
		}
	}

	/* header test */
	.theme-light div[role="dialog"]:not([class]) div[class^="header"] {
		background-image: linear-gradient(
			125deg,
			#fff4fa,
			#ffe7ff
		);
	}

