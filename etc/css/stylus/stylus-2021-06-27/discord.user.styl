	:root {
		--background-primary: var(--bg) !important;
		--background-secondary: var(--bg2) !important;
		--background-tertiary: var(--bg3) !important;
		--background-accent: var(--accent) !important;
	}
	.theme-dark {
		--background-primary: #030306 !important;
		--background-secondary: #111122 !important;
		--background-secondary-alt: #17172f !important;
		--background-tertiary: #1f1d3e !important;
		--background-accent: var(--background-secondary-alt) !important;
		--background-floating: var(--background-secondary) !important;
		--channeltextarea-background: var(--background-primary) !important;
		/* Custom */
		--background-left-sidebar: #000;
		--background-transparent-primary: #03030680;
		--background-secondary-gradient: /*#111622*/#16122b;
		/* Scrollbar */
		--scrollbar-background-primary: #101010;
		--scrollbar-background-hover: #202020;
		--scrollbar-background-active: #404040;
	}

	.theme-light {
		--background-floating: #fff !important;
		--background-secondary: #f7f8f9/*fafbfc*/ !important;
		--background-secondary-alt: #f1f2f4 !important;
		--background-tertiary: #f0f0f0 !important;
		--interactive-normal: #4f5c6a !important;
		--text-normal: #0f1113 !important;
		/* Custom */
		--background-left-sidebar: #c4c5c8;
		--background-transparent-primary: #ffffff80;
		--background-secondary-gradient: #fff4fa;
		/* Scrollbar */
		--scrollbar-background-primary: var(--background-secondary-alt);
		--scrollbar-background-hover: rgb(204,204,204);
		--scrollbar-background-active: rgb(180,180,180);
	}

	/* Partial Mountaineer colorscheme */
	.theme-dark {
		--hljs-color-0: #050505;
		--hljs-color-1: #ac8a8c;
		--hljs-color-2: #8aac8b;
		--hljs-color-3: #aca98a;
		--hljs-color-4: #8f8aac;
		--hljs-color-5: #ac8aac;
		--hljs-color-6: #8aabac;
		--hljs-color-7: #e7e7e7;
		--hljs-color-8: #676767;
		--hljs-color-9: #ac8a8c;
		--hljs-color-10: #8aac8b;
		--hljs-color-11: #aca98a;
		--hljs-color-12: #8f8aac;
		--hljs-color-13: #ac8aac;
		--hljs-color-14: #8aabac;
		--hljs-color-15: #f0f0f0;
	}
	.theme-light {
		--hljs-color-0: #363636;
		--hljs-color-1: #786162;
		--hljs-color-2: #617878;
		--hljs-color-3: #787661;
		--hljs-color-4: #646178;
		--hljs-color-5: #786178;
		--hljs-color-6: #617878;
		--hljs-color-7: #e7e7e7;
		--hljs-color-8: #676767;
		--hljs-color-9: #786162;
		--hljs-color-10: #617878;
		--hljs-color-11: #787661;
		--hljs-color-12: #646178;
		--hljs-color-13: #786178;
		--hljs-color-14: #617878;
		--hljs-color-15: #f0f0f0;
	}
	.hljs-comment,
	.hljs-quote {
		color: var(--hljs-color-8) !important;
	}
	.hljs-addition,
	.hljs-built_in,
	.hljs-deletion,
	.hljs-meta,
	.hljs-meta .hljs-keyword,
	.hljs-name,
	.hljs-section,
	.hljs-selector-class,
	.hljs-selector-id,
	.hljs-selector-tag,
	.hljs-title,
	.hljs-keyword {
		color: var(--hljs-color-1) !important;
	}
	.hljs-bullet,
	.hljs-doctag,
	.hljs-literal,
	.hljs-link,
	.hljs-meta .hljs-meta-string,
	.hljs-number,
	.hljs-regexp,
	.hljs-string,
	.hljs-subst,
	.hljs-symbol {
		color: var(--hljs-color-2) !important;
	}
	.c .hljs-keyword,
	.css .hljs-built_in {
		color: var(--hljs-color-3) !important;
	}
	.hljs-selector-pseudo {
		color: var(--hljs-color-4) !important;
	}
	.c .hljs-number,
	.hljs-variable {
		color: var(--hljs-color-5) !important;
	}
	.c .hljs-literal,
	.hljs-attr,
	.hljs-attribute,
	.hljs-selector-attr,
	.hljs-class .hljs-title,
	.hljs-template-variable,
	.hljs-type {
		color: var(--hljs-color-6) !important;
	}

	/* These don't use variables */
	div[class^="backdrop"] + div[class^="layer"] div[class^="root"] {
		background-color: var(--background-primary);
	}
	.theme-dark [class*="colorWhite"] {
		color: var(--text-normal) !important;
	}
	.theme-dark div[class^="slider"] > div[class^="bar"] {
		background-color: var(--interactive-muted) !important;
	}
	/*.theme-dark div[class^="message"]:not([class*="ErrorBar"]),*/
	div[class^="peopleColumn"],
	div[class^="uploadModal"] {
		background-color: var(--background-primary) !important;
	}
	.theme-dark div[class*="footer"] {
		box-shadow: inset 0 1px 0 var(--background-secondary) !important;
		background-color: var(--background-secondary) !important;
	}
	.theme-light div[class^="jumpToPresentBar"],
	.theme-light div[class^="headerPlaying"] {
		background-color: var(--logo-primary) !important;
	}

	/* Scrollbar */
	::-webkit-scrollbar-thumb {
		background-color: var(--scrollbar-background-primary) !important;
	}
	::-webkit-scrollbar-thumb:hover {
		background-color: var(--scrollbar-background-hover) !important;
	}
	::-webkit-scrollbar-thumb:active {
		background-color: var(--scrollbar-background-active) !important;
	}

	/**
	 * Start
	**/

	/* Remove elements */
	#message-tts,
	#user-context-message-user,
	#user-context-call,
	#user-context-note,
	#user-context-invite-to-server,
	a[href="/store"],
	a[href="https://support.discord.com"],
	#channels div[aria-label="Invite to Server"],
	button[aria-label="Send a gift"],
	div[aria-label^="Mute channel"],
	/* */
	form::before,
	div[class^="option"]::after,
	div[class^="option"][class*="selected"]::before,
	div[class^="children"]::after,
	//div[class^="resultsGroup"] svg,
	/* */
	div[class^="menu"] div[role="group"][class^="wrapper"],
	div[class^="protip"],
	div[class^="socialLinks"],
	div[class^="emptyChannelIcon"],
	div[class^="userPopout"] div[class^="footer"],
	div[class^="infoScroller"] div[class^="userInfoSection"]:first-child,
	div[class^="channelNotice"],
	div[class^="peopleColumn"] h2,
	div[class^="nowPlayingColumn"],
	div[aria-hidden="true"] div[class^="attachment"],
	#chat-messages > div[class^="wrapper"] div[class*="blob"]
	{
			display: none !important;
	}

	/* ACTUALLY stop animations */
	/* {
			-webkit-animation: none !important;
			animation: none !important;
			-webkit-transition: none !important;
			transition: none !important;
	}
	[class*="animating"] {
			all: unset !important;
	}

	/* Hide sidebar on small windows */
	@media only screen and (max-width: 768px) {
		// Chat
		div[class^="sidebar"] {
			width: 0 !important;

			nav[class^="wrapper"]:hover + div &,
			nav[class^="wrapper"] + div &:hover {
				width: 240px !important;
			}

			@media (prefers-reduced-motion: no-preference) {
				transition: var(--trans-dur);
			}
		}
		nav[class^="wrapper"] div[class^="scroller"] {
			box-shadow: inset -1px 0 var(--background-left-sidebar);
		}

		// Settings
		div[aria-label="USER_SETTINGS"] div[class^="sidebarRegion"]:not(:hover) {
			flex: 1 0 13px !important;
		}
	}

